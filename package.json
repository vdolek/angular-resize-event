    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.14"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "css-element-queries": "^1.0.1"
  }
